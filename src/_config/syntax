Start -> Prog  . 

Prog -> ClassList FuncDefList main FuncBody . 

ClassList -> ClassDecl ClassList 
	| .
ClassDecl -> class Sys(deny) id InheritList lcurbr Sys(deny) MembList rcurbr Sys(deny) semi Sys(deny) .

InheritList -> inherits Sys(deny) id InheritList2  
	| . 
InheritList2 -> comma Sys(deny) id InheritList2  
	| . 

MemberDecl -> FuncDecl 
	| VarDecl . 
MembList -> Visibility Sys(deny) MemberDecl MembList 
	| . 


AParams -> Expr AParams2 
	| . 
AParams2 -> comma Sys(deny) Expr AParams2  
	| . 


ArithExpr -> Term ArithExpr2  . 
ArithExpr2 -> AddOp Term Sys(ternary) ArithExpr2 
	| . 


Expr -> ArithExpr Expr2  . 
Expr2 -> RelOp ArithExpr Sys(ternary)
	| . 


Term -> Factor Term2  . 
Term2 -> MultOp Factor Sys(ternary) Term2
	| .


FuncOrVarList -> id FuncOrVarList2 . 
FuncOrVarList2 -> IndiceList Sys(Var) FuncOrVarList3
	| lpar Sys(deny) AParams rpar Sys(deny) Sys(FCall) FuncOrVarList3 . 
FuncOrVarList3 -> dot Sys(deny) id FuncOrVarList2 
	| . 


Factor -> FuncOrVarList 
	| intnum 
	| floatnum 
	| stringlit 
	| lpar Sys(deny) Expr rpar Sys(deny)
	| not Factor Sys(binary)
	| Sign Factor Sys(binary)
	| qm Sys(deny) lsqbr Sys(deny) Expr colon Sys(deny) Expr colon Sys(deny) Expr rsqbr Sys(deny) . 


DimList -> lsqbr Sys(deny) Intnum rsqbr Sys(deny) DimList
	| . 


VarDecl -> Type id DimList semi Sys(deny)  . 
VarDeclList -> VarDecl VarDeclList
	| . 


FParam -> Type id DimList .
FParamList -> FParam FParamList2 
	| .
FParamList2 -> comma Sys(deny) FParam FParamList2 
	| .


FuncDefList -> FuncDef FuncDefList
	| .
FuncDef -> func Sys(deny) id ScopeSpec Sys(scope) lpar Sys(deny) FParamList rpar Sys(deny) colon Sys(deny) FuncType Sys(deny) FuncBody .


FuncBody -> lcurbr Sys(deny) FuncBody2 StatementList rcurbr Sys(deny)  . 
FuncBody2 -> var Sys(deny) lcurbr Sys(deny) VarDeclList rcurbr Sys(deny)
	| . 


FuncDecl -> func Sys(deny) id lpar Sys(deny) FParamList rpar Sys(deny) colon Sys(deny) FuncType Sys(deny) semi Sys(deny)  . 


StatBlock -> lcurbr Sys(deny) StatementList rcurbr Sys(deny)
	| Statement
	| . 


StatementList -> Statement StatementList
	| . 


Statement -> FuncOrAssignList semi Sys(deny)
	| if Sys(deny) lpar Sys(deny) Expr rpar Sys(deny) then Sys(deny) StatBlock else Sys(deny) StatBlock semi Sys(deny)
	| while lpar Sys(deny) Expr rpar Sys(deny) StatBlock semi Sys(deny) Sys(left_root)
	| read lpar Sys(deny) VarList rpar Sys(deny) semi Sys(deny) Sys(left_root)
	| write lpar Sys(deny) Expr rpar Sys(deny) semi Sys(deny) Sys(left_root)
	| return lpar Sys(deny) Expr rpar Sys(deny) semi Sys(deny) Sys(left_root)
	| break semi Sys(deny) Sys(left_root)
	| continue semi Sys(deny) Sys(left_root) . 


FuncOrAssignList -> id FuncOrAssignList2  . 
FuncOrAssignList2 -> IndiceList Sys(Var) FuncOrAssignList3
	| lpar Sys(deny) AParams Sys(FCall) rpar Sys(deny) FuncOrAssignList4  . 
FuncOrAssignList3 -> dot Sys(deny) id FuncOrAssignList2
	| assign Expr Sys(ternary) . 
FuncOrAssignList4 -> dot Sys(deny) id FuncOrAssignList5
	| . 
FuncOrAssignList5 -> IndiceList Sys(Var) dot Sys(deny) id FuncOrAssignList5
	| lpar Sys(deny) AParams Sys(FCall) rpar Sys(deny) FuncOrAssignList6  . 
FuncOrAssignList6 -> dot Sys(deny) id FuncOrAssignList5
	| . 


IndiceList -> lsqbr Sys(deny) Expr rsqbr Sys(deny) IndiceList
	| . 

VarList -> id VarList2 . 
VarList2 -> IndiceList Sys(Var) VarList3  . 
VarList3 -> dot Sys(deny) id VarList2
	| . 


ScopeSpec -> sr Sys(deny) id 
	| .
RelOp -> eq
	| neq
	| lt
	| gt
	| leq
	| geq  . 
Sign -> plus
	| minus .
Type -> integer 
	| float
	| string 
	| id  . 
Intnum -> intnum
	| . 
MultOp -> mult
	| div
	| and .
Visibility -> public 
	| private
	|  . 
FuncType -> Type
	| void .
AddOp -> plus
	| minus
	| or .


Sys(deny) -> .
Sys(binary) -> .
Sys(ternary) -> .
Sys(Var) -> .
Sys(FCall) -> .
Sys(left_root) -> .
Sys(scope) -> .